# Moderation Service Configuration

# Server
NODE_ENV=development
PORT=5002

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Backend Webhook Configuration
BACKEND_WEBHOOK_URL=http://localhost:5001/api/webhooks/moderation
BACKEND_WEBHOOK_SECRET=your-webhook-secret-here

# Moderation Thresholds
MODERATION_SCORE_APPROVE_THRESHOLD=0.5
MODERATION_SCORE_REJECT_THRESHOLD=0.5
TEXT_EXTRACTION_MAX_CHARS=1000

# TensorFlow Model Configuration
TOXICITY_MODEL_THRESHOLD=0.7
